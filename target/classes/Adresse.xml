<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.ecommerce.metier.Adresse">
        <table name="adresse" schema="commerce"/>
        <attributes>
            <id name="ida">
                <column name="ida"/>
            </id>
            <basic name="adresse">
                <column name="adresse"/>
            </basic>
            <basic name="ville">
                <column name="ville"/>
            </basic>
            <basic name="codepostale">
                <column name="codepostale"/>
            </basic>
            <basic name="pays">
                <column name="pays"/>
            </basic>
            <basic name="idc">
                <column name="idc"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
